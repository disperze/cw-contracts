{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "anyOf": [
    {
      "description": "Returns the lock info",
      "type": "object",
      "required": [
        "get_lock"
      ],
      "properties": {
        "get_lock": {
          "type": "object",
          "required": [
            "address",
            "id"
          ],
          "properties": {
            "address": {
              "type": "string"
            },
            "id": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Returns the locks by address",
      "type": "object",
      "required": [
        "get_locks"
      ],
      "properties": {
        "get_locks": {
          "type": "object",
          "required": [
            "address"
          ],
          "properties": {
            "address": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
